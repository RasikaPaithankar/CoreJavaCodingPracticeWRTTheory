package com.java.practice.one;

public class FibonacciSequence {
	
	public static void fabonacciSeq(int n) {
		int a = 0;
		int b = 1;
		int c = 1;
		
		for(int i = 0; i <= n ; i++) {
			System.out.println(a + ", ");
			a = b;
			b = c;
			
		}
	}

	public static void main(String[] args) {
		fabonacciSeq(10);
	}

}
